    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.0.4",
    "@prisma/client": "^4.0.0",
    "@reduxjs/toolkit": "^1.8.3",
    "@remix-run/dev": "^1.6.3",
    "@remix-run/eslint-config": "^1.6.3",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/uuid": "^8.3.4",
    "argon2": "^0.28.5",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "eslint": "^8.15.0",
    "joi": "^17.6.0",
    "postcss": "^8.4.14",
